package com.Arraylist;

import java.util.ArrayList;
import java.util.List;

public class Collage {
	
public List<String> addstudentdetail() {

	Student s = new Student();

	s.id = 1;
	s.name = "mauli";
	s.address = "pune";

	Student s1 = new Student();

	s1.id = 2;
	s1.name = "suraj";
	s1.address = "rajuri";

	Student s2 = new Student();

	s2.id = 3;
	s2.name = "dnyaneshwar";
	s2.address = "a.nager";
	
	List<String> liststudent = new ArrayList<String>();
	
	liststudent.add(s);
	liststudent.add(s1);
	liststudent.add(s2);
	
	return liststudent;

}
}