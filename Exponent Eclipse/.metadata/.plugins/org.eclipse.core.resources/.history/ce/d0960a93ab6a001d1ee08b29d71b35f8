package com.multipalcatchblock;

public class MultipleCatchBlockExample {

	public static void main(String[] args) {
		
		System.out.println("=== main methd===");
		
	   try {
		   
		   Float f = new Float[1];
		   System.out.println(" valu of float :"+f);
		   
				Integer i = new Integer("mauli");
				System.out.println("valu of i"+ i);
				
			} catch (NumberFormatException e) {
				System.err.println("catch block");
				e.printStackTrace();
			}
		
	}

	}


